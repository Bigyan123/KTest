 <settings>
    <servers>
        <server>
            <id>ossrh</id>
           <username>${OSS_USERNAME}</username>
            <password>${OSS_PASSWORD}</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>ossrh</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <gpg.executable>gpg</gpg.executable>
                 <gpg.passphrase>${GPG_PASSPHRASE}</gpg.passphrase>
            </properties>
       </profile>
    </profiles>
</settings>